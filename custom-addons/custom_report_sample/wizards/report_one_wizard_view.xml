<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="report_one_wizard_view_form" model="ir.ui.view">
            <field name="name">report_one_wizard_view_form</field>
            <field name="model">custom_report_sample.report_one_wizard</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <h1>Report One Wizard</h1>
                        <group>
                            <field name="content"/>
                        </group>
                    </sheet>
                    <footer>
                        <button type="object" name="action_print" string="Print" class="oe_highlight"/>
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window id="report_one_wizard_action" name="Report One" res_model="custom_report_sample.report_one_wizard"
                    view_mode="form" target="new" multi="False"/>

        <menuitem id="report_one_wizard_menu" name="Report One" parent="sales_team.menu_base_partner" action="custom_report_sample.report_one_wizard_action" sequence="100"/>

    </data>
</odoo>